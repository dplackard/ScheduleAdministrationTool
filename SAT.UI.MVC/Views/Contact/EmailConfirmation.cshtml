@model SAT.UI.MVC.Models.ContactViewModel

@{
    ViewBag.Title = "Email Confirmation";
}

<section class="hero-wrap hero-wrap-2" style="background-image: url('../../Content/images/bg_1.jpg');">
    <div class="overlay"></div>
    <div class="container">
        <div class="row no-gutters slider-text align-items-center justify-content-center">
            <div class="col-md-9 ftco-animate text-center">
                <h1 class="mb-2 bread">@ViewBag.Title</h1>
                <p class="breadcrumbs"><span class="mr-2"><a href="@Url.Action("Index", "Home")">Home <i class="ion-ios-arrow-forward"></i></a></span> <span>@ViewBag.Title <i class="ion-ios-arrow-forward"></i></span></p>
            </div>
        </div>
    </div>
</section>
<div>
    <dl class="dl-horizontal">
        <dt class="text-center">
            @Html.DisplayNameFor(model => model.Name)
        </dt>

        <dd class="text-center">
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt class="text-center">
            @Html.DisplayNameFor(model => model.Email)
        </dt>

        <dd class="text-center">
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt class="text-center">
            @Html.DisplayNameFor(model => model.Subject)
        </dt>

        <dd class="text-center">
            @Html.DisplayFor(model => model.Subject)
        </dd>

        <dt class="text-center">
            @Html.DisplayNameFor(model => model.Message)
        </dt>

        <dd class="text-center">
            @Html.DisplayFor(model => model.Message)
        </dd>

    </dl>
</div>
